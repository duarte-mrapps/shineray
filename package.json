{
  "name": "appdaloja",
  "version": "1.25.001",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "patch-package",
    "build": "yarn react-native build-android --mode release",
    "setup": "sh init.sh",
    "setup:icons": "sh initIcons.sh",
    "clean": "sh clean.sh",
    "prints:iphone:maestro": "sh checkBuildForPrintsMaestro.sh --iphone",
    "prints:ipad:maestro": "sh checkBuildForPrintsMaestro.sh --ipad",
    "prints:android-phone:maestro": "sh checkBuildForPrintsMaestro.sh --android-phone",
    "prints:android-tablet:maestro": "sh checkBuildForPrintsMaestro.sh --android-tablet",
    "prints:ios:build": "sh checkBuildForPrintsMaestro.sh --iphone && sh checkBuildForPrintsMaestro.sh --ipad",
    "prints:android:build": "sh checkBuildForPrintsMaestro.sh --android-phone && sh checkBuildForPrintsMaestro.sh --android-tablet",
    "prints:build": "yarn prints:ios:build && yarn prints:android:build",
    "prints:iphone": "sh checkBuildForPrints.sh --ios & killall -9 Terminal & osascript -e 'tell application \"Terminal\" to do script \"cd '$(pwd)' && yarn start\"' & wait && detox test --configuration iphone.debug e2e/iphone.test.js && sh organize.sh iphone && xcrun simctl shutdown all",
    "prints:ipad": "sh checkBuildForPrints.sh --ios & killall -9 Terminal & osascript -e 'tell application \"Terminal\" to do script \"cd '$(pwd)' && yarn start\"' & wait && detox test --configuration ipad.debug e2e/ipad.test.js && sh organize.sh ipad && xcrun simctl shutdown all",
    "prints:phone": "sh checkBuildForPrints.sh --android & killall -9 Terminal & osascript -e 'tell application \"Terminal\" to do script \"cd '$(pwd)' && yarn start\"' & wait && detox test --configuration android.debug e2e/android.test.js --headless; sh organize.sh android; adb emu kill || adb devices | grep emulator | awk '{print $1}' | xargs -I {} adb -s {} emu kill; adb start-server; adb devices | grep emulator | awk '{print $1}' | xargs -I {} adb -s {} emu avd wipe-data",
    "prints:tablet": "sh checkBuildForPrints.sh --android & killall -9 Terminal & osascript -e 'tell application \"Terminal\" to do script \"cd '$(pwd)' && yarn start\"' & wait && detox test --configuration tablet.debug e2e/tablet.test.js --headless; sh organize.sh tablet; adb emu kill || adb devices | grep emulator | awk '{print $1}' | xargs -I {} adb -s {} emu kill; adb start-server; adb devices | grep emulator | awk '{print $1}' | xargs -I {} adb -s {} emu avd wipe-data",
    "prints:ios": "yarn prints:iphone && yarn prints:ipad",
    "prints:android": "yarn prints:phone && yarn prints:tablet",
    "prints": "yarn prints:ios && yarn prints:android",
    "update:app": "sh update.sh"
  },
  "dependencies": {
